<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Selenium Framework Test Suite" parallel="tests" thread-count="2">
    <parameter name="useGrid" value="false"/>
    <test name="Login and Dropdown Tests">
        <classes>
            <class name="com.selenium.framework.tests.LoginTest"/>
            <class name="com.selenium.framework.tests.DropdownTest"/>
        </classes>
    </test>
</suite> 